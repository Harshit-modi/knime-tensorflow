<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         point="org.knime.core.PortType">
      <portType
            color="#8fb3c2"
            hidden="false"
            name="TensorFlow Deep Learning Network"
            objectClass="org.knime.dl.tensorflow.base.portobjects.DLTensorFlowNetworkPortObject"
            objectSerializer="org.knime.dl.tensorflow.base.portobjects.DLTensorFlowNetworkPortObject$Serializer"
            specClass="org.knime.dl.tensorflow.base.portobjects.DLTensorFlowNetworkPortObjectSpec"
            specSerializer="org.knime.dl.tensorflow.base.portobjects.DLTensorFlowNetworkPortObjectSpec$Serializer">
      </portType>
   </extension>
   <extension
         point="org.knime.workbench.repository.categories">
      <category
            icon="icons/category.png"
            level-id="tensorflow"
            locked="true"
            name="TensorFlow"
            path="/labs/deeplearning">
      </category>
   </extension>
   <extension
         point="org.knime.workbench.repository.nodes">
      <node
            category-path="/labs/deeplearning/tensorflow"
            deprecated="false"
            factory-class="org.knime.dl.tensorflow.base.nodes.reader.DLTensorFlowReaderNodeFactory">
      </node>
  </extension>
</plugin>
